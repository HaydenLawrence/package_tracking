{
  "name": "package_tracking",
  "version": "0.0.1",
  "description": "Web app to tracking packages from a variety of couriers",
  "main": "index.html",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-in-memory-web-api": "0.0.14",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.6",
    "systemjs": "^0.19.31",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "tsc": "^1.20150623.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.1"
  },
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run dev\"",
    "dev": "lite-server -c config/bs-config.json",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HaydenLawrence/package_tracking.git"
  },
  "keywords": [
    "package",
    "tracking"
  ],
  "author": "Hayden Lawrence",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/HaydenLawrence/package_tracking/issues"
  },
  "homepage": "https://github.com/HaydenLawrence/package_tracking#readme",
  "jspm": {
    "directories": {
      "baseURL": "web"
    },
    "configFile": "config/config.js",
    "dependencies": {
      "ts": "github:frankwallis/plugin-typescript@^5.0.8"
    },
    "devDependencies": {
      "typescript": "npm:typescript@^1.6.2"
    }
  }
}
